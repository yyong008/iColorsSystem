{
  "name": "icolors-vars",
  "version": "0.0.6",
  "description": "System-level Color System Css Variables, fast and happy to use it",
  "main": "./dist/antd.css",
  "type": "module",
  "scripts": {
    "dev": "esno src/antd.ts && esno src/elm-plus/index.ts && esno src/tailwind/index.ts && esno src/material-ui/index.ts",
    "csso": "gulp",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx .",
    "lint:fix": "eslint --ext .ts,.js,.tsx,.jsx . --fix",
    "stylelint": "stylelint ./src/**/*.{css,less,scss,sass}",
    "stylelint:fix": "stylelint ./src/**/*.{css,less,scss,sass} --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "postinstallmac": "git config core.hooksPath .husky && chmod 777 .husky/*",
    "commit": "git add . && cz"
  },
  "keywords": [],
  "author": "magesium",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "18.8.1",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-prettier": "2.0.0",
    "ts-node": "10.9.1",
    "typescript": "^4.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "libs/",
    "dist/",
    "README.md",
    "README-CN.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yyong008/iColorsSystem.git",
    "branch": "master",
    "platform": "github"
  }
}
